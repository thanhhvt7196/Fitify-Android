<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string"/>
    <attr name="image" format="reference"/>
    <attr name="subtitle" format="string"/>
    <attr name="showSeparator" format="boolean"/>
    <attr name="borderColor" format="color"/>

    <declare-styleable name="ProfileMyJourneyItemView">
        <attr name="title"/>
        <attr name="image"/>
        <attr name="count" format="string"/>
    </declare-styleable>

    <declare-styleable name="ProfileHeaderStatView">
        <attr name="title"/>
        <attr name="image"/>
        <attr name="subtitle"/>
    </declare-styleable>

    <declare-styleable name="SettingItemView">
        <attr name="title"/>
        <attr name="image"/>
        <attr name="showSeparator"/>
    </declare-styleable>

    <declare-styleable name="OnboardingItemWithImageView">
        <attr name="title"/>
        <attr name="image"/>
        <attr name="borderColor"/>
    </declare-styleable>
</resources>